library(DECIPHER)
dna = readDNAStringSet("idtaxa_train_0.fasta",format="fasta")
s <- strsplit(names(dna), " ")
taxonomy <- sapply(s, `[`, 2)
trainingSet <- LearnTaxa(dna, taxonomy)
test = readDNAStringSet("test_0.fasta",format="fasta")
ids <- IdTaxa(test,trainingSet, threshold = 1, processors = 48, strand = "top")
options(max.print = .Machine$integer.max)
sink("idtaxa.out")
print.default(ids)
sink()
